{
  "name": "wakapi_discord",
  "version": "0.0.2",
  "description": "[Wakapi](https://github.com/muety/wakapi) integration in the [BetterDiscord](https://github.com/rauenzi/BetterDiscordApp) client. Enables you to track the time you spend answering support tickets.",
  "main": "config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev:init:local": "git clone https://github.com/rauenzi/BDPluginLibrary.git && cp config.local.json ./BDPluginLibrary && mv ./BDPluginLibrary/config.local.json ./BDPluginLibrary/config.json",
    "dev:init:remote": "git clone https://github.com/rauenzi/BDPluginLibrary.git && cp config.remote.json ./BDPluginLibrary && mv ./BDPluginLibrary/config.remote.json ./BDPluginLibrary/config.json",
    "dev:build": "node ./BDPluginLibrary/scripts/build.js local Wakapi"
  },
